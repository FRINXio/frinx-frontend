{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": ["lint", "type-check", "typegen", "codegen", "build"]
      }
    }
  },
  "targetDefaults": {
    "lint": {},
    "type-check": {
      "dependsOn": [
        {
          "projects": ["@frinx/shared"],
          "target": "^typegen"
        }
      ]
    },
    "typegen": {
      "outputs": ["{projectRoot}/dist"]
    },
    "codegen": {
      "outputs": ["{projectRoot}/__generated__"]
    },
    "build": {
      "outputs": ["{projectRoot}/dist", "{workspaceRoot}/build-client"]
    }
  }
}
