{
  "name": "@frinx/api",
  "version": "0.0.20",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Frinx.io",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development node --no-warnings=ExperimentalWarning ./bin/dev.mjs",
    "build:esbuild": "NODE_ENV=production node --no-warnings=ExperimentalWarning ./bin/build.mjs",
    "build": "run-s build:esbuild typegen",
    "typegen": "tsc --declaration --esModuleInterop true --target ESNext --moduleResolution node --outDir dist src/index.ts",
    "lint": "eslint --ext .ts 'src/**/*.ts' --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "fp-ts": "2.13.1",
    "io-ts": "2.2.20",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@frinx/eslint-config-typescript": "19.0.4-build.1",
    "@frinx/shared": "workspace:^",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@urql/exchange-retry": "1.0.0",
    "chalk": "^5.3.0",
    "esbuild": "^0.19.2",
    "eslint": "8.36.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "execution-time": "^1.4.1",
    "ora": "^7.0.1",
    "typescript": "4.9.5",
    "urql": "3.0.3"
  }
}
