{
  "name": "@frinx/resource-manager",
  "version": "0.0.1",
  "private": true,
  "module": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@frinx/shared": "workspace:^",
    "@urql/exchange-retry": "1.0.0",
    "ace-builds": "1.4.14",
    "chakra-ui-autocomplete": "^1.4.5",
    "feather-icons-react": "0.6.2",
    "formik": "2.2.9",
    "graphql": "15.8.0",
    "ipaddr.js": "^2.0.1",
    "lodash": "4.17.21",
    "match-sorter": "6.3.1",
    "urql": "3.0.3",
    "uuid": "8.3.2",
    "yup": "0.32.11"
  },
  "scripts": {
    "dev": "webpack serve --mode development",
    "lint": "eslint --ext .ts,.tsx src",
    "type-check": "tsc --noEmit",
    "generate:declaration": "tsc --emitDeclarationOnly",
    "build:webpack": "NODE_ENV=production webpack --mode production",
    "build": "run-s build:webpack generate:typings",
    "formatter:check": "prettier -l '**/*.ts' '**/*.tsx' '**/*.js' '**/*.jsx' '**/*.json' '**/*.md'",
    "formatter": "prettier --write '**/*.ts' '**/*.tsx' '**/*.js' '**/*.jsx' '**/*.json' '**/*.md'",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:clean": "rm -r src/__generated__",
    "typegen": "tsc --declaration --jsx react --esModuleInterop true --target ESNext --moduleResolution node --outDir dist yup.d.ts src/index.ts"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@chakra-ui/react": "1.8.9",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@frinx/eslint-config-typescript": "19.0.4-build.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.0",
    "@graphql-codegen/typescript-operations": "2.4.1",
    "@types/react": "17.0.42",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@webpack-cli/serve": "1.7.0",
    "babel-loader": "8.3.0",
    "css-loader": "5.2.7",
    "eslint": "8.34.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "framer-motion": "6.2.8",
    "graphql-tag": "2.12.6",
    "html-webpack-plugin": "5.5.0",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.2",
    "style-loader": "2.0.0",
    "typescript": "4.8.4",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "framer-motion": "^4.1.3 || ^6.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  }
}
