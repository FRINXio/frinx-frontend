{
  "name": "@frinx/resource-manager",
  "version": "0.0.1",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@frinx/shared": "workspace:^",
    "@urql/exchange-retry": "1.0.0",
    "chakra-ui-autocomplete": "^1.4.5",
    "feather-icons-react": "0.6.2",
    "formik": "2.2.9",
    "graphql": "15.8.0",
    "ipaddr.js": "^2.0.1",
    "lodash": "4.17.21",
    "match-sorter": "6.3.1",
    "urql": "3.0.3",
    "uuid": "9.0.0",
    "yup": "0.32.11"
  },
  "scripts": {
    "dev": "NODE_ENV=development node --no-warnings=ExperimentalWarning ./bin/dev.mjs",
    "build": "NODE_ENV=production node --no-warnings=ExperimentalWarning ./bin/build.mjs",
    "lint": "eslint --ext .ts,.tsx src",
    "type-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:clean": "rm -r src/__generated__",
    "typegen": "tsc --declaration --jsx react --esModuleInterop true --target ESNext --moduleResolution node --outDir dist yup.d.ts src/index.ts"
  },
  "devDependencies": {
    "@chakra-ui/react": "2.5.1",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@frinx/build-scripts": "workspace:^",
    "@frinx/eslint-config-typescript": "19.0.4-build.1",
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.8",
    "@graphql-codegen/typescript-operations": "2.5.13",
    "@types/react": "17.0.53",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "framer-motion": "6.2.8",
    "graphql-tag": "2.12.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.9.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.6.7 || ^2.0.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.0.0",
    "framer-motion": "^4.1.3 || ^6.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  }
}
