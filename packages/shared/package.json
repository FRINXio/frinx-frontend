{
  "name": "@frinx/shared",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "module": "src/index.ts",
  "typings": "dist/index.d.ts",
  "author": "Frinx.io",
  "files": [
    "dist/*"
