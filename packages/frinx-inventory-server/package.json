{
  "name": "@frinx/inventory-server",
  "version": "0.0.1",
  "engines": {
    "node": "^14.15.0"
  },
  "author": "Frinx.io",
  "license": "MIT",
  "scripts": {
    "build:babel": "cross-env NODE_ENV=production babel --extensions .ts ./src --out-dir ./build/babel",
    "build:pack": "cross-env NODE_ENV=production ncc build build/babel/server.js -o build/pack",
    "build:clear": "rimraf build",
    "build": "run-s build:clear build:babel build:pack",
    "dev": "cross-env DEBUG='frinx-inventory-server' DEBUG_COLORS=true DOTENV_CONFIG_PATH=./.env nodemon --exec 'node -r dotenv/config -r ./babel-register-ts.js' -- ./src/server.ts",
    "lint": "eslint --ext .ts 'src/**/*.ts' --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.13.16",
    "@frinx/eslint-config-typescript-base": "^18.2.1-build.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@vercel/ncc": "^0.28.6",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "pino-pretty": "^5.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/express": "^4.17.12",
    "@types/pg": "^8.6.0",
    "@types/pino": "^6.3.8",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "pino": "^6.11.3"
  }
}
