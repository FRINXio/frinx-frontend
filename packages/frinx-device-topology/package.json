    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@types/react": "17.0.65",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "7.2.0",
