{
  "name": "@frinx/uniresource-ui",
  "version": "0.0.1",
  "private": true,
  "module": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@urql/exchange-retry": "0.3.2",
    "ace-builds": "1.4.14",
    "chakra-ui-autocomplete": "1.4.5",
    "feather-icons-react": "0.5.0",
    "formik": "2.2.9",
    "graphql": "15.8.0",
