    "eslint-plugin-react-hooks": "4.3.0",
    "framer-motion": "6.2.8",
    "graphql-tag": "2.12.6",
    "html-webpack-plugin": "5.5.0",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.2",
    "style-loader": "2.0.0",
    "typescript": "4.6.3",
    "webpack": "5.51.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "3.11.3"
  },
  "peerDependencies": {
    "@chakra-ui/icons": "^1.0.15",
